{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": [
            1,
            4
        ],
        "no-console": "off",
        "no-plusplus": "off",
        "prefer-const": "off",
        "max-len": [
            1,
            125
        ],
        "no-useless-return": "off"
    }
}